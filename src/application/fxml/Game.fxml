<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="720.0" prefWidth="1280.0" styleClass="gameBg" stylesheets="@../css/Game.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.Game">
   <children>
      <Button fx:id="submitButton" defaultButton="true" layoutX="473.0" layoutY="480.0" mnemonicParsing="false" onAction="#submit" prefHeight="95.0" prefWidth="286.0" text="Enter" textFill="#092c47">
         <font>
            <Font name="Operating instructions" size="50.0" />
         </font>
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
      <Label fx:id="progressLabel" layoutX="234.0" layoutY="242.0" prefHeight="53.0" prefWidth="301.0" text="Label" textFill="#4a3909">
         <font>
            <Font name="OpenDyslexicAlta" size="36.0" />
         </font></Label>
      <Button fx:id="listenButton" layoutX="62.0" layoutY="468.0" mnemonicParsing="false" onAction="#listen" prefHeight="69.0" prefWidth="92.0" textFill="#775241">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
      <Button id="dontknowButton" fx:id="skipButton" layoutX="858.0" layoutY="468.0" mnemonicParsing="false" onAction="#skip" prefHeight="95.0" prefWidth="286.0" text="skip" textFill="#4d120c">
         <font>
            <Font name="Operating instructions" size="40.0" />
         </font>
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
      <Button fx:id="nextButton" layoutX="473.0" layoutY="480.0" mnemonicParsing="false" onAction="#next" prefHeight="95.0" prefWidth="286.0" text="next -&gt;">
         <font>
            <Font name="Operating instructions" size="50.0" />
         </font>
      </Button>
      <Label fx:id="topicLabel" layoutX="400.0" layoutY="118.0" prefHeight="81.0" prefWidth="544.0" text="Topic" textFill="#8a1919">
         <font>
            <Font name="OpenDyslexicAlta" size="45.0" />
         </font>
      </Label>
      <Button id="home" layoutX="1157.0" layoutY="545.0" mnemonicParsing="false" onAction="#menu" prefHeight="69.0" prefWidth="92.0">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
      <ChoiceBox fx:id="speedSelector" layoutX="172.0" layoutY="554.0" prefHeight="49.0" prefWidth="85.0">
         <effect>
            <DropShadow>
               <color>
                  <Color red="0.10559999942779541" green="0.3199999928474426" blue="0.14374946057796478" />
               </color>
            </DropShadow>
         </effect></ChoiceBox>
      <Label layoutX="66.0" layoutY="556.0" text="Speed:" textFill="#45751e">
         <font>
            <Font name="OpenDyslexicAlta" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
