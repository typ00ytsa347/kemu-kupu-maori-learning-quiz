<AnchorPane prefHeight="720.0" prefWidth="1280.0" styleClass="menuBg" stylesheets="@../css/Menu.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.Menu">
